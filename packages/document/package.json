{
  "name": "@dxos/document",
  "version": "1.0.0-beta.3",
  "description": "CRDT yjs-based document",
  "homepage": "https://github.com/dxos/editor#readme",
  "bugs": {
    "url": "https://github.com/dxos/editor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dxos/editor.git"
  },
  "license": "GPL-3.0",
  "main": "dist/es/index.js",
  "scripts": {
    "build": "npm run clean && babel ./src --out-dir ./dist/es --ignore \"**/*.test.js\" --source-maps inline",
    "build:watch": "npm run build --watch",
    "clean": "rm -rf dist",
    "lint": "eslint ./src --ignore-path ../../.eslintignore",
    "test": "jest --rootDir ./src --verbose --passWithNoTests",
    "prepublishOnly": "npm run build && npm run doc",
    "doc": "jsdoc2md --plugin dmd-readable --template ./doc/README.hbs --files ./src/*.js > ./README.md"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "hast-util-to-mdast": "^7.1.0",
    "lib0": "^0.2.26",
    "mdast-util-to-hast": "^9.1.0",
    "rehype-document": "^5.0.0",
    "rehype-format": "^3.0.1",
    "rehype-parse": "^6.0.2",
    "rehype-remark": "^8.0.0",
    "rehype-sort-attributes": "^3.0.1",
    "rehype-stringify": "^7.0.0",
    "remark-breaks": "^1.0.5",
    "remark-parse": "^8.0.2",
    "remark-rehype": "^7.0.0",
    "remark-stringify": "^8.0.0",
    "to-vfile": "^6.1.0",
    "unified": "^9.0.0",
    "yjs": "^13.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "dmd-readable": "^1.2.1",
    "jest": "^24.8.0",
    "jsdoc-to-markdown": "^5.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "52fe112f144e28ce4878ddb70255affef177894f"
}
